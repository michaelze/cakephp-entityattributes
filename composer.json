{
    "name": "michaelze/cakephp-entityattributes",
    "description": "Configurable Entity Attributes plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "keywords": ["cakephp", "entity", "attribute"],
    "homepage": "https://github.com/michaelze/cakephp-entityattributes",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "EntityAttributes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EntityAttributes\\Test\\": "tests",
            "TestApp\\": "tests/test_app",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests"
        }
    }
}
